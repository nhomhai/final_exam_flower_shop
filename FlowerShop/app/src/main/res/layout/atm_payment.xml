<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bac"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/button7"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="22dp"
            android:background="@drawable/donga"
            android:onClick="donga"/>

        <Button
            android:id="@+id/button8"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/button7"
            android:layout_centerHorizontal="true"
            android:background="@drawable/vcb"
            android:onClick="vcb"/>

        <Button
            android:id="@+id/button9"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/button7"
            android:background="@drawable/vtb"
            android:onClick="vtb"/>

        <Button
            android:id="@+id/button10"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="141dp"
            android:background="@drawable/vib"
            android:onClick="vib"/>

        <Button
            android:id="@+id/button11"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/button10"
            android:layout_centerHorizontal="true"
            android:background="@drawable/exim"
            android:onClick="exim"/>

        <Button
            android:id="@+id/button12"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/button10"
            android:background="@drawable/vp"
            android:onClick="vp"/>

        <Button
            android:id="@+id/button13"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="241dp"
            android:background="@drawable/sc"
            android:onClick="sacom"/>

        <Button
            android:id="@+id/button14"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/button13"
            android:layout_centerHorizontal="true"
            android:background="@drawable/hd"
            android:onClick="hdb"/>

        <Button
            android:id="@+id/button15"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/button13"
            android:background="@drawable/mrt"
            android:onClick="mrt"/>

        <Button
            android:id="@+id/button16"
            android:layout_width="130dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="120dp"
            android:background="@drawable/tech"
            android:onClick="tech"/>
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>